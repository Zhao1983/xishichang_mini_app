/**app.wxss**/
@import './components/weui-miniprogram/style/weui.wxss';

page {
    width: 100%;
    height: 100%;
    margin: 0 auto;
    padding: 0 !important;
    line-height: 1.2;
    font-family: 'PingFang Regular', sans-serif !important;
    background-color: #f5f6fd !important;
    -webkit-text-size-adjust: 100% !important;
    -moz-text-size-adjust: 100% !important;
    -ms-text-size-adjust: 100% !important;
}

::-webkit-scrollbar {
    display: none;
}

.text-center {
    text-align: center !important;
}

.text-right {
    text-align: right !important;
}

.text-through {
    text-decoration: line-through;
}

.cart-position {
    position: fixed;
    bottom: 75px;
    left: 20px;
    z-index: 1000;
}

.cart-position .cart {
    position: relative;
    display: flex;
}

.cart-position .cart .cart-icon {
    width: 36px;
    height: 36px;
}

.cart-position .cart .cart-badge {
    border-radius: 50%;
    width: 15px;
    height: 15px;
    background-color: #ff3543;
    border: 1px solid #fff;
    position: absolute;
    display: flex;
    align-items: center;
    justify-content: center;
    top: -5px;
    right: -4px;
}

.cart-position .cart .cart-badge .text {
    font-size: 9px;
    font-weight: bold;
    color: #fff;
}

.menu-position {
    position: fixed;
    bottom: 75px;
    right: 20px;
}

.menu-position .menu-icon {
    width: 42px;
    height: 42px;
}

.bg-mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    z-index: 700;
    transition: all 260ms ease;
}

.goods-calc-content {
    height: auto;
    margin-top: 10px;
    text-align: center;
}

.goods-calc-content.w100 {
    width: 100px;
}

.goods-calc-content .weight-info {
    font-size: 14px;
    color: #222222;
}

.goods-calc-content .quantity-info {
    width: 100%;
    height: 30px;
    display: flex;
    align-items: center;
    border: 1px solid #e4e4e4;
    margin-top: 5px;
}

.goods-calc-content .quantity-info .item-content {
    display: flex;
    align-items: center;
    justify-content: center;
    border-right: 1px solid #e4e4e4;
    height: 100%;
}

.goods-calc-content .quantity-info .item-content:last-child {
    border-right: none;
}

.goods-calc-content .quantity-info .item-content.w30 {
    width: 30%;
}

.goods-calc-content .quantity-info .item-content.w40 {
    width: 40%;
}

.goods-calc-content .quantity-info .item-content .minus-img {
    width: 10px;
    height: 1px;
}

.goods-calc-content .quantity-info .item-content .plus-img {
    width: 10px;
    height: 10px;
}

.goods-calc-content .quantity-info .item-content .input-num {
    width: 100%;
    height: 100%;
    border: none;
    color: #222222;
    font-size: 15px;
    text-align: center;
}

.search-tool {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    padding: 0px 20px;
    height: 60px;
    background-color: #f5f6fd;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 100;
}

.search-input {
    width: 100%;
    height: 40px;
    border-radius: 50px;
    border: 1px solid #ffca2f;
    background-color: #fff;
    position: relative;
}

.search-input .icon-search {
    width: 17px;
    height: 17px;
    position: absolute;
    top: 12px;
    left: 8px;
}

.search-input .icon-cancel {
    width: 17px;
    height: 17px;
    position: absolute;
    top: 12px;
    right: 8px;
}

.search-input .input {
    height: 100%;
    font-size: 15px;
    color: #777777;
    padding: 0px 30px;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
}

.more-detail {
    margin: 1em 0;
    text-align: center;
}

.more-detail .text {
    font-size: 12px;
    color: #222222;
    font-weight: bold;
}

.delivery-tool {
    position: fixed;
    left: 0;
    right: 0;
    bottom: -30px;
    z-index: 800;
    height: 0px;
    background-color: #fff;
    transition: all 260ms ease;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    padding: 15px;
}

.delivery-tool.collapsed {
    height: 190px;
    bottom: 0;
}

.delivery-tool .header {
    display: flex;
    align-items: center;
    margin-bottom: 10px;
}

.delivery-tool .header .left-item {
    width: 95%;
    text-align: center;
    font-size: 17px;
    color: #222222;
    font-weight: bold;
}

.delivery-tool .header .right-item {
    width: 5%;
    width: 17px;
    height: 17px;
    text-align: right;
}

.delivery-tool .tool-row {
    padding: 15px;
    border-bottom: 1px solid #eeeeee;
    display: flex;
    align-items: center;
}

.delivery-tool .tool-row .left-item {
    width: 41.66%;
    font-size: 15px;
    color: #222222;
}

.delivery-tool .tool-row .right-item {
    width: 58.33%;
    font-size: 15px;
    color: #cf000e;
    text-decoration: underline;
}

.help-tool {
    position: fixed;
    width: 100%;
    bottom: -1px;
    z-index: 800;
    height: 0px;
    background-color: #fff;
    transition: all 260ms ease;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
}

.help-tool.collapsed {
    height: 60%;
    bottom: 0;
}

.help-tool .header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 15px;
}

.help-tool .header .title {
    font-size: 17px;
    color: #222222;
    font-weight: bold;
}

.help-tool .header .icon-cancel {
    width: 17px;
    height: 17px;
}

.help-tool .content {
    height: auto;
}

.help-tool .content .help-item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 15px 25px;
    border-bottom: 1px solid #e3e3e3;
}

.help-tool .content .help-item:last-child {
    border: none;
}

.help-tool .content .help-item .text {
    font-size: 15px;
    color: #222222;
}

.help-tool .content .help-item .text.active {
    color: #cf000e;
}

.help-tool .footer {
    height: 45px;
    display: flex;
    align-items: center;
    justify-content: center;
}

.help-tool .footer .btn-confirm {
    width: 200px;
    height: 100%;
    border-radius: 30px;
    font-size: 15px;
    color: #222222;
    font-weight: bold;
    background-color: #ffcc33;
    display: flex;
    align-items: center;
    justify-content: center;
}

.dialog-phone {
    position: fixed;
    top: 30%;
    background: #fff;
    z-index: 800;
    width: 85%;
    left: 7.5%;
    margin-left: 0;
    border-radius: 10px;
    height: auto;
}

.dialog-phone .header {
    padding: 15px;
    background-color: #ffcc33;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    font-size: 16px;
    color: #222222;
    font-weight: bold;
    text-align: center;
}

.dialog-phone .content {
    padding: 0px 15px;
    height: auto;
}

.dialog-phone .content .item {
    position: relative;
    padding: 15px 0px;
    border-bottom: 1px solid #e3e3e3;
}

.dialog-phone .content .input {
    font-size: 15px;
    color: #222222;
    border: none;
}

.dialog-phone .content .auth-btn {
    position: absolute;
    right: 0;
    top: 11px;
    width: 100px;
    height: 30px;
    background-color: #f0f4f3;
    font-size: 13px;
    color: #bebebe;
    border-radius: 5px;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 900;
}

.dialog-phone .content .auth-btn.active {
    color: #222222;
    background-color: #ffcc33;
}

.dialog-phone .footer {
    padding: 25px 15px;
}

.dialog-phone .footer .btn-confirm {
    border-radius: 30px;
    padding: 12px;
    background-color: #ededed;
    border: 1px solid #eaeaea;
    color: #8c8c8c;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
}

.dialog-phone .footer .btn-confirm.active {
    border: 1px solid #ffcc33;
    color: #222222;
    background-color: #ffcc33;
}

.free-price-item {
    font-size: 11px;
    color: #fff;
    padding: 3px;
    border-radius: 3px;
    display: table;
}

.free-price-item.red {
    background-color: #ff7e4c;
}

.free-price-item.green {
    background-color: #37c2a8;
}

